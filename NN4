#define  _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
#include<string.h>
#include<windows.h>
#include<time.h>
#include<math.h>
#include<stdlib.h>


int main()
{
   int a = 1;	
   int i = 1;
	for (i = 1; i <= 9; i++)
	{
		for (a = 1; a <= i; a++)
		{
			printf("%d*%d=%d ",a,i,a*i);
		}
		printf("\n");
	}

	return 0;
}
